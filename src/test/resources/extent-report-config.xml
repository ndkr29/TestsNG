<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>

    <!-- report theme -->
    <!-- STANDARD, DARK -->
    <theme>DARK</theme>

    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <!-- HTTP, HTTPS -->
    <protocol>HTTPS</protocol>

    <!-- offline report -->
    <timelineEnabled>true</timelineEnabled>
    
    <!-- offline report -->
    <enableOfflineMode>false</enableOfflineMode>
    
    <!-- use thumbnails for base64 images -->
    <!-- this may slowdown viewing tests -->
    <thumbnailForBase64>false</thumbnailForBase64>

    <!-- title of the document -->
    <documentTitle>DOC Tile</documentTitle>

    <!-- report name - displayed at top-nav -->
    <reportName>Report NameXML 5</reportName>

    <!-- timestamp format -->
    <timeStampFormat>dd-MM-YYYY hh:mm:ss</timeStampFormat>

  
  <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $document.getElementsByClassName('logo')[0].style.display='none';
            ]]>
        </scripts>

    <!-- custom styles -->
    <style>
 
		}
  </style>

  </configuration>
</extentreports>